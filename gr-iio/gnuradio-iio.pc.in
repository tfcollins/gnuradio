prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/@GR_LIBRARY_DIR@
includedir=${prefix}/@GR_INCLUDE_DIR@

Name: gnuradio-iio
Description: GNU Radio blocks for the IIO devices
Requires: gnuradio-runtime gnuradio-blocks
Version: @GR_IIO_VERSION@
Libs: -L${libdir} -lgnuradio-iio -lgnuradio-blocks
Cflags: -I${includedir}
